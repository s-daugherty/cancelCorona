{

"manifest_version": 2,
    "name": "Rewind 2020",
    "description": "Extension to replace instances of coronavirus/covid with user specified word!",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
